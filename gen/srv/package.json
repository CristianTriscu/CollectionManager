{
    "name": "CollectionManager",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^6",
        "@sap/cds-dk": "^7",
        "@sap/cds-hana": "^2",
        "@sap/xssec": "^3",
        "express": "^4",
        "passport": "^0"
    },
    "devDependencies": {
        "@sap/cds-dk": "^7",
        "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
        "@sap/ux-specification": "^1.108.16",
        "@sapui5/ts-types": "^1.92.2",
        "eslint": "^7.32.0",
        "rimraf": "^3.0.2",
        "sqlite3": "^5.0.4"
    },
    "scripts": {
        "start": "NODE_OPTIONS='--max-http-header-size=80000' node_modules/@sap/cds/bin/cds.js run",
        "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy CollectionManager --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
        "watch-lasttry": "cds watch --open lasttry/webapp/index.html?sap-ui-xx-viewCache=false"
    },
    "cds": {
        "hana": {
            "deploy-format": "hdbtable"
        },
        "requires": {
            "db": {
                "kind": "hana-cloud"
            }
        }
    },
    "sapux": [
        "app/test-01",
        "app/project1",
        "app/lasttry"
    ]
}